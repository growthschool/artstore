<table class="table table-hover">
  <thead>
    <th>#</th>
    <th>Product Pic</th>
    <th>Name</th>
    <th>Price</th>
    <th>Option</th>
  </thead>
  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.id %></td>
        <td></td>
        <td><%= product.title %></td>
        <td><%= product.price %></td>
        <td><%= link_to("Edit", edit_admin_product_path(product), class: "btn") %></td>
      </tr>
    <% end %>
  </tbody>
</table>
