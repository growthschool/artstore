<%= link_to("回到產品列表", admin_products_path) %>

<h2>產品明細</h2>

<%= simple_form_for [:admin, @product] do |f| %>

<div class="group">
  <%= f.input :title %>
</div>

<div class="group">
  <%= f.input :description %>
</div>

<div class="group">
  <%= f.input :quantity %>
</div>

<div class="group">
  <%= f.input :price %>
</div>

<% end %>
