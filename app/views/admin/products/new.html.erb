<h3>新增商品</h3>
<%= simple_form_for [:admin, @product] do |f| %>

<div class="form-group">
  <%= f.input :title %>
</div>
<div class="form-group">
  <%= f.input :description %>
</div>
<div class="form-group">
  <%= f.input :quantity %>
</div>
<div class="form-group">
  <%= f.input :price %>
</div>



<div class = "form-group">
  <%= f.simple_fields_for :photos do |c| %>
    <%= c.input :image ,as: :file %>
  <% end %>

</div>

<%= f.submit "Submit", data: {disable_with: "submitting...."} %>


<% end %>
