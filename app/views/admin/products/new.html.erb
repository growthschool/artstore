<%= simple_form_for [:admin, @product] do |f| %>
  <%= f.input :title, label: "標題", type: "text" %>
  <%= f.input :description, label: "敘述", type: "textarea" %>
  <%= f.input :quantity, label: "數量", type: "number" %>
  <%= f.input :price, label: "價錢", type: "number" %>
  <%= f.button :submit %>
<% end %>
