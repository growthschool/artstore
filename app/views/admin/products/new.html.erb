<%= simple_form_for [:admin, @product] do |f| %>
  <%= f.input :title, label: "標題" %>
  <%= f.input :description, label: "敘述" %>
  <%= f.input :quantity, label: "數量" %>
  <%= f.input :price, label: "價錢" %>
  <%= f.simple_fields_for :photos do |c| %>
  	<%= c.input :image, label: "上傳商品圖片", as: :file %>
  <% end  %>
  <%= f.button :submit, "新增商品", :disable_with => 'Submiting...' %>
<% end %>