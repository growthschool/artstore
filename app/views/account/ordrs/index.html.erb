<h2> 訂單列表 </h2>

<table class="table table-bordered"
  <thead>
    <tr>
      <th>#</th>
      <th>生成時間</th>
    </tr>
  </thead>
  <tbody>
    <% @order.each do |order| %>
    <tr>
      <td><%= link_to(order.id, order_path(order.token)) %></td>
      <td><%= order.create_at.to_s(:long) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
